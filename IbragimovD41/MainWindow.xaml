<Window x:Class="IbragimovD41.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IbragimovD41"
        mc:Ignorable="d"
        Title="41 Размер ИбрагимовД ИСП-3322" Height="400" Width="700"
        Icon="image/icon.ico"
        FontFamily="Comic Sans MS"
        >
    <Grid Background="#FFFFCC99">
        <Grid.RowDefinitions>
            <RowDefinition Height="75" ></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>

        <Image Source="image/logo.png" HorizontalAlignment="Left"></Image>

        <TextBlock Text="41 Размер" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>

        <Button Background="#FFCC6600" Content="Назад" Name="BtnBack" Click="BtnBack_Click" HorizontalAlignment="Right" Height="30" Width="175" Margin="5" VerticalAlignment="Center"></Button>

        <Frame Grid.Row="1" Background="#FFFFFF" Name="MainFrame" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered"></Frame>
        
        
    </Grid>
</Window>
